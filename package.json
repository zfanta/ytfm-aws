{
  "name": "ytfm",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts~",
  "scripts": {
    "test": "eslint src --ext .ts",
    "run:get": "serverless invoke local -f pubsubhubbubHttpGet --path src/functions/api/pubsubhubbub/http.get.mock.json",
    "run:updateEmailTemplates": "sls invoke local -f pubsubhubbubHttpPost",
    "deploy:dev": "serverless deploy --stage dev -v",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.4.1",
    "@aws-sdk/client-sesv2": "^3.5.0",
    "@aws-sdk/client-sqs": "^3.4.1",
    "@middy/core": "^1.5.0",
    "@middy/http-json-body-parser": "^1.5.0",
    "node-fetch": "^2.6.1",
    "serverless-dynamodb-local": "^0.2.39",
    "source-map-support": "^0.5.19",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.16.0",
    "@types/aws-lambda": "^8.10.64",
    "@types/node": "^14.14.6",
    "@types/node-fetch": "^2.5.8",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "4",
    "eslint": "7",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "fork-ts-checker-webpack-plugin": "^6.0.0",
    "json-schema-to-ts": "^1.4.1",
    "pre-commit": "^1.2.2",
    "serverless": "^2.16.0",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^8.0.10",
    "ts-node": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-node-externals": "^2.5.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
