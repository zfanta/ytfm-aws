{
  "name": "ytfm",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "scripts": {
    "test": "node --max-old-space-size=8192 node_modules/eslint/bin/eslint.js . --ext .ts",
    "parcel:serve": "copyfiles -f src/frontend/robots.txt dist/frontend/serve && parcel --no-cache --target frontend --dist-dir dist/frontend/serve src/frontend/index.html",
    "parcel:serve:email": "parcel -p 1235 --no-cache --target frontend --dist-dir dist/frontend/email src/libs/emailTemplate.html",
    "parcel:build": "copyfiles src/frontend/robots.txt dist/frontend/build && parcel build --no-cache --no-source-maps --no-scope-hoist --target frontend --dist-dir dist/frontend/build src/frontend/index.html",
    "run:get": "serverless invoke local -f pubsubhubbubHttpGet --path src/functions/api/pubsubhubbub/http.get.mock.json",
    "run:updateEmailTemplates": "sls invoke local -f updateEmailTemplates",
    "run:sendEmail": "sls invoke local -f sendEmail --path src/functions/email/send.mock.json",
    "deploy:dev": "serverless deploy --stage dev -v",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --fix",
    "serverless:offline": "sls offline",
    "webpack:serve": "webpack serve --config webpack.frontend.dev.js",
    "webpack:build": "copyfiles -f src/frontend/robots.txt dist/frontend/build && webpack build --config webpack.frontend.prod.js"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.18.0",
    "@aws-sdk/client-lambda": "^3.18.0",
    "@aws-sdk/client-sesv2": "^3.18.0",
    "@aws-sdk/client-sqs": "^3.18.0",
    "@aws-sdk/util-dynamodb": "^3.20.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@middy/core": "^2.4.1",
    "@middy/http-json-body-parser": "^2.4.1",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "cookie": "^0.4.1",
    "dayjs": "^1.10.5",
    "ical-generator": "^2.2.0",
    "iso8601-duration": "^1.3.0",
    "libsodium-wrappers": "^0.7.9",
    "nanoid": "^3.1.23",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.2",
    "process": "^0.11.10",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^8.32.0",
    "react-lazy-load-image-component": "^1.5.1",
    "recoil": "^0.3.1",
    "serverless-dynamodb-local": "^0.2.39",
    "source-map-support": "^0.5.19",
    "stream-browserify": "^3.0.0",
    "validator": "^13.6.0",
    "winston": "^3.3.3",
    "wouter": "^2.7.4",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.46.0",
    "@types/aws-lambda": "^8.10.77",
    "@types/js-yaml": "^4.0.1",
    "@types/libsodium-wrappers": "^0.7.9",
    "@types/node": "^15.12.4",
    "@types/node-fetch": "^2.5.10",
    "@types/nodemailer": "^6.4.2",
    "@types/query-string": "^6.3.0",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.8",
    "@types/react-lazy-load-image-component": "^1.5.1",
    "@types/swagger-ui-react": "^3.35.1",
    "@types/validator": "^13.1.4",
    "@typescript-eslint/eslint-plugin": "4",
    "browserslist": "^4.16.6",
    "copyfiles": "^2.4.1",
    "css-loader": "^5.2.6",
    "esbuild-loader": "^2.13.1",
    "eslint": "7",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "5",
    "eslint-plugin-react": "^7.24.0",
    "fork-ts-checker-webpack-plugin": "^6.2.10",
    "fullstack-serverless": "^0.8.0",
    "html-webpack-plugin": "^5.3.1",
    "json-schema-to-ts": "^1.6.4",
    "mini-css-extract-plugin": "^1.6.0",
    "parcel": "2.0.0-beta.1",
    "pre-commit": "^1.2.2",
    "serverless": "^2.47.0",
    "serverless-offline": "^7.0.0",
    "serverless-webpack": "^5.5.0",
    "style-loader": "^2.0.0",
    "swagger-ui-react": "^3.50.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.4",
    "webpack": "^5.39.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "targets": {
    "frontend": {
      "engines": {
        "browsers": "last 1 Chrome version"
      }
    }
  }
}
