{
  "name": "ytfm",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "scripts": {
    "test": "node --max-old-space-size=8192 node_modules/eslint/bin/eslint.js . --ext .ts",
    "parcel:serve": "parcel --no-cache --target frontend --dist-dir dist/frontend/serve src/frontend/index.html",
    "parcel:serve:email": "parcel -p 1235 --no-cache --target frontend --dist-dir dist/frontend/email src/libs/emailTemplate.html",
    "parcel:build": "parcel build --no-cache --no-source-maps --no-scope-hoist --target frontend --dist-dir dist/frontend/build src/frontend/index.html",
    "run:get": "serverless invoke local -f pubsubhubbubHttpGet --path src/functions/api/pubsubhubbub/http.get.mock.json",
    "run:updateEmailTemplates": "sls invoke local -f updateEmailTemplates",
    "run:sendEmail": "sls invoke local -f sendEmail --path src/functions/email/send.mock.json",
    "deploy:dev": "serverless deploy --stage dev -v",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --fix",
    "serverless:offline": "sls offline"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.18.0",
    "@aws-sdk/client-lambda": "^3.17.0",
    "@aws-sdk/client-sesv2": "^3.17.0",
    "@aws-sdk/client-sqs": "^3.17.0",
    "@aws-sdk/util-dynamodb": "^3.18.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@middy/core": "^2.2.0",
    "@middy/http-json-body-parser": "^2.2.0",
    "cookie": "^0.4.1",
    "dayjs": "^1.10.4",
    "ical-generator": "^2.2.0",
    "iso8601-duration": "^1.3.0",
    "libsodium-wrappers": "^0.7.9",
    "nanoid": "^3.1.23",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.1",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recoil": "^0.3.1",
    "serverless-dynamodb-local": "^0.2.39",
    "source-map-support": "^0.5.19",
    "validator": "^13.6.0",
    "winston": "^3.3.3",
    "wouter": "^2.7.4",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.43.0",
    "@types/aws-lambda": "^8.10.76",
    "@types/js-yaml": "^4.0.1",
    "@types/libsodium-wrappers": "^0.7.9",
    "@types/node": "^15.6.1",
    "@types/node-fetch": "^2.5.10",
    "@types/nodemailer": "^6.4.2",
    "@types/query-string": "^6.3.0",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/swagger-ui-react": "^3.35.1",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "4",
    "eslint": "7",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "5",
    "eslint-plugin-react": "^7.24.0",
    "fork-ts-checker-webpack-plugin": "^6.2.10",
    "fullstack-serverless": "^0.8.0",
    "js-yaml": "^4.1.0",
    "json-schema-to-ts": "^1.6.4",
    "parcel": "2.0.0-beta.1",
    "pre-commit": "^1.2.2",
    "serverless": "^2.43.1",
    "serverless-offline": "^7.0.0",
    "serverless-webpack": "^5.5.0",
    "swagger-ui-react": "^3.49.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-node-externals": "^3.0.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "targets": {
    "frontend": {
      "engines": {
        "browsers": "last 1 Chrome version"
      }
    }
  }
}
